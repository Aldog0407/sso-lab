# Optional: Override default URLs (useful for production)
# REDIRECT_URI=http://localhost:3000/callback
# POST_LOGOUT_REDIRECT_URI=http://localhost:3000

# API Configuration
# API_URL=http://localhost:4000
# API_TIMEOUT=5000
